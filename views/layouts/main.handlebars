          <button id="logout-btn" type="button" class="btn btn-primary mx-1">Logout</button>
          {{else}}
          <button id="login-btn" type="button" class="btn btn-primary mx-1">Login</button>
          {{/if}}
        </div>
      </div>
    </nav>
  </header>
  {{{body}}}
  <script src="/jquery/jquery.js"></script>
  <script src="/js/bootstrap.min.js"></script>
  <script src="./js/main.js"></script>
