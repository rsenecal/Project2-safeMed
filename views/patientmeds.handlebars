<div class="container">
    
    <div class="col-md-6 float-xxl-end">
                <div>
                    <a href="#" data-bs-toggle="modal" data-bs-target=".add-new" class="btn btn-sm btn-light float-xxl-end"><i class="bx bx-plus me-1"></i>Add Prescription</a>
                </div>
    </div>
    <div class="row align-items-center">
        <div class="col-md-6">
            <div class="mb-3">
                <h5 class="card-title">Current Prescription List</h5>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col-lg-12">
                <div class="table-responsive">
                    <table class="table project-list-table table-nowrap align-middle table-borderless">
                        <thead>
                            <tr>
                                <th scope="col" class="ps-4" style="width: 50px;">
                                    <div class="form-check font-size-16"><input type="checkbox" class="form-check-input" id="contacusercheck" /><label class="form-check-label" for="contacusercheck"></label></div>
                                </th>
                                <th scope="col">Med Name</th>
                                <th scope="col">Dosage</th>
                                <th scope="col">Quantity</th>
                                <th scope="col">Dispensed on</th>
                                <th scope="col">Started On</th>
                                <th scope="col">Ended On</th>
                                <th scope="col" style="width: 200px;">Action</th>
                            </tr>
                        </thead>
                          {{#each patientmeds as |patientmed|}} 
                        <tbody>
                            {{#each patientmed.Meds as |med| }}
                                {{!-- {{#each med.prescription as |prescription|}} --}}
                              <tr>
                                <th scope="row" class="ps-4">
                                    <div class="form-check font-size-16"><input type="checkbox" class="form-check-input" id="contacusercheck1" /><label class="form-check-label" for="contacusercheck1"></label></div>
                                </th>
                                <td class="pt-lname">{{med.name}}</td>
                                <td class="pt-lname">{{prescription.dose}}</td>
                                <td class="pt-lname">{{prescription.quantity}}</td>
                                <td class="pt-dob">{{formatDate prescription.dispense_date 'MMM Do YYYY'}}</td>
                                <td class="pt-dob">{{formatDate prescription.date_started 'MMM Do YYYY'}}</td>
                                {{#has_passed prescription.date_ended}}
                                <td class="pt-dob">{{formatDate prescription.date_ended 'MMM Do YYYY'}}</td>
                                 {{else}}
                                <td class="pt-dob" style="color:red;">{{formatDate prescription.date_ended 'MMM Do YYYY'}}</td>
                               
                                {{/has_passed}}
                                <td>
                                    <ul class="list-inline mb-0">
                                        <li class="list-inline-item">
                                            <a href="javascript:void(0);" data-bs-toggle="tooltip" data-bs-placement="top" title="Edit" class="px-2 text-primary"><i class="bx bx-pencil font-size-18"></i></a>
                                        </li>
                                        <li class="list-inline-item">
                                            <a href="javascript:void(0);" data-bs-toggle="tooltip" data-bs-placement="top" title="Archive" class="px-2 text-primary"><i class="bx bx-trash font-size-18"></i></a>
                                        </li>
                                        {{#has_passed prescription.date_ended}}
                                        {{else}}
                                        <li class="list-inline-item">
                                            <a href="javascript:void(0);" data-bs-toggle="tooltip" data-bs-placement="top" title="Warning:Prescription Expired" class="px-2 text-primary"><i class="bx bx-error-alt font-size-18"></i></a>
                                        </li>
                                        {{/has_passed}}

                                    </ul>
                                </td>
                            {{/each}}
                            </tr>
                        </tbody>
                        {{/each }} 
                    </table>
                </div>
        </div>
    </div>
